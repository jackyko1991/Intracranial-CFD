FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    vessel
    {
        // type fixedValue;
        // value  uniform (0 0 0);
        type            noSlip;
    }

    ICA
    {
        type fixedValue;
        value uniform (-0.1408e-3 0.3251e-3 0.5325e-3);
    }

    MCA
    {
        type inletOutlet;
        inletValue uniform (0 0 0) ;
        value uniform (0 0 0) ;
    }

    ACA
    {
        type inletOutlet;
        inletValue uniform (0 0 0) ;
        value uniform (0 0 0) ;
    }

    walls
    {
        type empty;
    }
}

